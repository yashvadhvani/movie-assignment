    "schemas": {
      "LoginDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@example.com",
            "description": "Email address"
          },
          "password": {
            "type": "string",
            "example": "strongPassword123",
            "description": "User password"
          }
        },
        "required": [
          "email",
          "password"
        ]
      },
      "RegisterDto": {
        "type": "object",
        "properties": {
          "email": {
            "type": "string",
            "example": "user@example.com",
            "description": "Email address"
          },
          "password": {
            "type": "string",
            "example": "strongPassword123",
            "description": "User password"
          },
